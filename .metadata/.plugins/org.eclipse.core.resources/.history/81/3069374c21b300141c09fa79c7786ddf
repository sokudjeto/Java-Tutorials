
public class LoopApp {
	public static void main(String[] args) {
		
		boolean loop = 6 < 1;
		
		int value = 0;
		
		boolean nloop = value >20;
		
		System.out.println(loop);
		System.out.println(nloop);
		
		while (value < 10){
		
			System.out.println("Hello" + " " + value);
			
		value = value + 1;
		}
	}
}
 