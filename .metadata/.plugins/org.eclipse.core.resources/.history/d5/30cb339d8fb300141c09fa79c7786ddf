public class IfConditionalApp {
	public static void main(String[] args) {

		boolean cond = 1 < 6;
		boolean cond2 = 5 != 4;
		boolean cond3 = 4 == 3;

		if( 4 == 4){
			System.out.println("Yes, it is true.");
		}
		System.out.println(cond);
		System.out.println(cond2);
		System.out.println(cond3);
		
	}

}
